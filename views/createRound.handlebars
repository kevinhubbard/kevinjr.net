<div id="createRoundView">
	<h1>Create A Round!</h1>
	<form action="/golfcard/rounds/create" method="post">
		<select name='course'>
			{{#each crs}}
				<option value="{{this.dataValues.courseID}}">{{this.dataValues.courseName}}</option>
			{{/each}}
		</select>
		{{!-- <input type="text" name="user" value="{{usr}}"> --}}
		<button id="usrHostLink" name="{{pid}}">{{usr}}</button>
		<input type="submit" value="Submit">
	</form>
</div>