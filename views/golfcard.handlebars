<div id="main">
	<h1>Golfcard App</h1>
	<div id="app">
		<div id="options">
			<button id="newRound">New Round</button>
			<button id="lookupRound">Lookup Round</button>
		</div>
		<div id="gamePanel">
			<div id="startInfo">
				<label for="courseName">Course Name:</label>
				<input type="text" id="courseName" name="courseName"><br>
				<input type="radio" id="9holes" name="holes" value="9">
				<label for="9holes">9 Holes</label><br>
				<input type="radio" id="18holes" name="holes" value="18">
				<label for="18holes">18 Holes</label><br>
				<button id="startRound">Start Round</button>
			</div>
		</div>
		<div id="showRound">
			<p>show rounds</p>
		</div>
	</div>
</div>